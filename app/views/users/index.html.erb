<table>
  <tr>
    <th>Email</th>
    <th>Name</th>
    <th>Show</th>
  </tr>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.first_name %> <%= user.last_name %></td>
        <td><%= link_to 'Show', user_path(user) %></td>
      </tr>
  <% end %>
</table>
<hr />
<%= link_to 'Add a new User', new_user_path %>